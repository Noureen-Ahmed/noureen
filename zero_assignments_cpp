ASSIGN 3
name        [ Valid | Good Practice ]
NAME        [ Valid | Bad Practice ]
1name       [ invalid ] => start with number
__name      [ valid ] => underscore
name@name   [ invalid ] => @ special character
name10name  [ valid] => number between
name!name   [ invalid ] => ! special character
first_NAME  [ valid ] => underscore
first_name  [ valid] => underscore
firstName   [ valid ] => no space
first name  [ invalid ] => space between
fn          [ invalid ] => reserved word
public      [ invalid] => reserved word
Public      [ valid ] =>  not reserved word
