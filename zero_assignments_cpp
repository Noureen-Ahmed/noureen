int calcspecial(int x, int y)
{
	if (x == y) {
		return x + y;
	}
	else if (x > y) {
		return x - y;
	}
	else if (x < y) {
	   return y - x;
	}

}
int main()
{
	cout << calcspecial(40, 40) << "\n";
	cout << calcspecial(200, 50) << "\n";
	cout << calcspecial(100, 300) << "\n";
	return 0;
}
ASSIGN 2
double money(float x, int y)
{
	int weeks = y / 7;
	int holiday = weeks * 2;
	double needed = (x / ( y - holiday));
	return needed;
}
int main()
{
	cout << money(2015, 21) << "\n";
	cout << money(4500, 40) << "\n";
}
